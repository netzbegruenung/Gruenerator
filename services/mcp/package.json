{
  "name": "@gruenerator/mcp",
  "version": "2.0.0",
  "description": "Gruenerator MCP Server - Suche in Grünen Parteiprogrammen (DE/AT)",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "gruenerator-mcp": "./src/index.ts"
  },
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "tsx --test src/**/*.test.ts"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "gruene",
    "green-party",
    "qdrant",
    "vector-search",
    "rag"
  ],
  "author": "Moritz Wächter",
  "license": "MIT",
  "dependencies": {
    "@gruenerator/shared": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@qdrant/js-client-rest": "^1.16.2",
    "cors": "^2.8.6",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@gruenerator/eslint-config": "workspace:*",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^22.19.11",
    "eslint": "^9.39.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
