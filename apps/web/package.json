  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(@testing-library|@adobe)/)"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    }
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "start:debug": "cross-env NODE_OPTIONS=\"--inspect --max-old-space-size=4096\" vite",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=2048 vite build",
    "build:low-memory": "cross-env NODE_OPTIONS=\"--max-old-space-size=1536 --gc-interval=100\" vite build",
    "build:profile": "cross-env NODE_OPTIONS=--prof vite build",
    "build:sequential": "cross-env NODE_OPTIONS=--max-old-space-size=2048 npm run build:clean && npm run build:chunks",
    "build:clean": "rimraf build && npm run deps:clean",
    "build:chunks": "cross-env NODE_OPTIONS=--max-old-space-size=2048 vite build --mode=production",
    "preview": "vite preview",
    "test": "jest --passWithNoTests",
    "analyze": "vite-bundle-analyzer build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build:analyze": "npm run build && npm run analyze",
    "deps:check": "npx depcheck",
    "deps:clean": "npm prune && npm dedupe"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}