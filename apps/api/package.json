{
  "name": "@gruenerator/api",
  "version": "1.0.0",
  "description": "Backend for Gruenerator project",
  "type": "module",
  "main": "server.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "start:backend": "PORT=3001 npx tsx server.ts",
    "start": "npx tsx server.ts",
    "dev": "cross-env HOCUSPOCUS_ENABLED=true npx tsx watch server.ts",
    "setup:authentik-sources": "node scripts/setupAuthentikSources.js",
    "setup:authentik-sources:dry-run": "node scripts/setupAuthentikSources.js --dry-run",
    "setup:authentik-sources:force": "node scripts/setupAuthentikSources.js --force",
    "start:dev": "cross-env NODE_ENV=development npm run start:backend",
    "start:dev:no-yjs": "cross-env NODE_ENV=development YJS_ENABLED=false npm run start:backend",
    "start:hocuspocus": "npx tsx ../../services/hocuspocus/src/index.ts",
    "start:prod": "cross-env NODE_ENV=production npm run start:backend",
    "test:embedding": "node scripts/testEmbedding.mjs",
    "download-whisper": "npx nodejs-whisper download",
    "offboarding": "node scripts/offboarding-cron.js",
    "offboarding:status": "node -e \"require('./services/offboardingService').OffboardingService.validateConfig(); console.log('âœ“ Offboarding configuration is valid');\"",
    "backfill:previews": "node scripts/backfillContentPreviews.mjs",
    "load:bundestag": "node scripts/loadBundestagContent.js",
    "load:bundestag:force": "FORCE_REINDEX=true node scripts/loadBundestagContent.js",
    "sync:bundestag": "node -e \"import('./services/bundestagSyncService.js').then(m => m.getBundestagSyncService().incrementalSync().then(r => { console.log(JSON.stringify(r, null, 2)); process.exit(r.success ? 0 : 1); }))\"",
    "sync:austria": "node scripts/syncAustrianSocialMedia.js",
    "sync:austria:dry-run": "node scripts/syncAustrianSocialMedia.js --dry-run",
    "sync:austria:full": "node scripts/syncAustrianSocialMedia.js --limit 100",
    "scrape:satzungen": "node scripts/scrape-satzungen.js",
    "scrape:satzungen:force": "node scripts/scrape-satzungen.js --force",
    "scrape:satzungen:stats": "node scripts/scrape-satzungen.js --stats",
    "export:finetuning": "npx tsx scripts/exportFineTuningData.ts",
    "export:finetuning:dry-run": "npx tsx scripts/exportFineTuningData.ts --dry-run",
    "export:finetuning:test": "npx tsx scripts/exportFineTuningData.ts --limit 10"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/mistral": "^3.0.0",
    "@ai-sdk/openai": "^3.0.0",
    "@anthropic-ai/sdk": "^0.71.0",
    "@blocknote/xl-ai": "^0.46.2",
    "@gruenerator/hocuspocus": "workspace:*",
    "@gruenerator/shared": "workspace:*",
    "@langchain/core": "^1.0.2",
    "@langchain/langgraph": "^1.0.1",
    "@langchain/mistralai": "^1.0.4",
    "@mistralai/mistralai": "^1.7.2",
    "@napi-rs/canvas": "^0.1.88",
    "@qdrant/js-client-rest": "^1.13.0",
    "@sentry/node": "^9.27.0",
    "@tus/file-store": "^1.5.1",
    "@tus/server": "^1.10.2",
    "@types/archiver": "^7.0.0",
    "ai": "^6.0.0",
    "archiver": "^7.0.1",
    "axios": "^1.13.5",
    "body-parser": "^1.20.3",
    "cheerio": "^1.0.0",
    "compression": "^1.7.5",
    "connect-redis": "^8.1.0",
    "cookie": "^1.1.1",
    "cors": "^2.8.5",
    "crawlee": "^3.14.1",
    "docx": "^9.5.1",
    "domhandler": "^5.0.3",
    "dotenv": "^17.0.0",
    "express": "^5",
    "express-session": "^1.18.2",
    "fastembed": "^1.14.4",
    "form-data": "^4.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jose": "^6.1.3",
    "jsonrepair": "^3.13.1",
    "langchain": "^1.0.2",
    "marked": "^17.0.0",
    "mem0ai": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "openai": "^4.102.0",
    "openid-client": "^6.1.2",
    "passport": "^0.7.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^4.10.38",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "redis": "^5.0.0",
    "sharp": "^0.34.5",
    "turndown": "^7.2.0",
    "uuid": "^11.0.5",
    "winston": "^3.17.0",
    "yjs": "^13.6.29",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@gruenerator/eslint-config": "workspace:*",
    "@types/body-parser": "^1.19.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/passport": "^1.0.17",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.5",
    "@types/turndown": "^5.0.6",
    "cross-env": "^7.0.3",
    "eslint": "^9.23.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
